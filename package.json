{
  "name": "robot-img",
  "version": "1.0.0",
  "description": "robot-img",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "BABEL_ENV=test jest --detectOpenHandles --forceExit --verbose --coverage",
    "test:last": "BABEL_ENV=test jest --detectOpenHandles --forceExit --verbose --coverage --changedSince origin/master",
    "test:clear": "BABEL_ENV=test jest --clearCache",
    "test:debug": "BABEL_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "BABEL_ENV=test jest --watch --verbose --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykan/robot-img.git"
  },
  "keywords": [
    "lazyload",
    "img"
  ],
  "author": "ykan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ykan/robot-img/issues"
  },
  "homepage": "https://github.com/ykan/robot-img#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.29.0",
    "eslint-config-alloy": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": ">=6",
    "jest": "^27.0.6",
    "lint-staged": ">=10",
    "prettier": "2.3.2",
    "react-test-renderer": "^17.0.2",
    "snowpack": "^3.7.1",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
